{
    "UseInMemoryDatabase": false,
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "NCacheSettings": {
        "CacheName": "studyporterCache",
        "EnableLogs": false,
        "EnableDetailLogs": false,
        "ExceptionsEnabled": false,
        "OperationRetry": 0,
        "operationRetryInterval": 0
    },
    "IdentityServer": {
        "Clients": {
            "StudyPorter": {
                "Profile": "IdentityServerSPA"
            }
        }
    },
    "AllowedHosts": "*"
}
